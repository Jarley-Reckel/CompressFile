{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "MyDebugger: C/C++: g++ build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/jarley/ED/TP3/TP/bin/main",
            "args": [
                "exemplos/input.txt",
                "exemplos/output.txt",
                "-c"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CompileFiles"
        }
    ]
}